<?xml version="1.0" encoding="UTF-8"?>
<codesense version="1.0">

    <!-- A completion behavior can customize the behavior of a completion when 
         it's selected in the completion list. If a behavior is defined in the 
         root <codesense> element, it will be applied to all completion sets 
         inside (unless more specific behaviors are defined in the sets). -->
    
	<behavior>
		<confirm characters=";" />
    </behavior>
	
	<set name="zado.as3.global.comment">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into>\n ${1} \n \*\/</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="/*" />
		<completion string="/**" />
    </set>
	
	<set name="zado.as3.access">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> $0</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="public" />
		<completion string="private" />
		<completion string="protected" />
		<completion string="dynamic" />
		<completion string="override" />
		<completion string="static" />
		<completion string="final" />
		
    </set>

	<set name="zado.as3.class">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> ${1:name} ${2:extends ${3:SuperClass}} ${4:implements ${5:Interface}} &$123;\n\t${6:}\n&$125;</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="class" />
    </set>
	
	<set name="zado.as3.function">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> ${1:name}(${2:})${3:\:void} &$123;\n\t${4:}\n&$125;</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="function" />
    </set>
	
	<!-- Control Statements -->
	<set name="zado.as3.control">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> (${1:expression}) &$123;\n\t${2:}\n&$125;</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="switch" />
		<completion string="if" />
		<completion string="else if" />
		<completion string="while" />
		<completion string="for each" />
    </set>
	
	<set name="zado.as3.control.case">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> ${1:constant} \n\t${2:}\nbreak;\n${4:}</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="case" />
		
    </set>

	<set name="zado.as3.control.case.default">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into>:\n\t${2:}\nbreak;</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="default" />
		
    </set>
	
	<set name="zado.as3.control.for">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> (${1:var i:uint = 0}; ${2:i &$60; 10}; ${3:i++}) &$123;\n\t${4:}\n&$125;</transform-into>
			</append-dynamic>
			
		</behavior>

		<completion string="for" />

    </set>
	
	<set name="zado.as3.control.dowhile">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> &$123;\n\t${1:}\n&$125; while(${2:expression});</transform-into>
			</append-dynamic>
			
		</behavior>

		<completion string="do" />

    </set>
	
	<set name="zado.as3.control.else">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\s*:)|[^:]</matched-suffix>
				<transform-into> &$123;\n\t${1:}\n&$125;</transform-into>
			</append-dynamic>
			
		</behavior>
		
		<completion string="else" />
		
    </set>
	
	
	
	
	
	
	
	
	
	
	
	
